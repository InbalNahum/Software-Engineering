<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.CustomerLoginController">
   <children>
      <AnchorPane fx:id="anchor_pane" layoutX="10.0" layoutY="10.0" prefHeight="245.0" prefWidth="319.0">
         <children>
            <GridPane layoutX="33.0" layoutY="109.0" prefHeight="65.0" prefWidth="259.0">
               <children>
                  <Label prefHeight="21.0" prefWidth="88.0" text="Customer Id:" />
                  <Label text="Car Number:" GridPane.rowIndex="1" />
                  <TextField fx:id="id_tf" GridPane.columnIndex="1" />
                  <PasswordField fx:id="carNumber_tf" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="111.0" minWidth="10.0" prefWidth="76.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="157.0" minWidth="10.0" prefWidth="157.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
            </GridPane>
            <Label layoutX="60.0" layoutY="42.0" prefHeight="35.0" prefWidth="200.0" text="Customer Login">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button fx:id="signIn_btn" layoutX="225.0" layoutY="206.0" mnemonicParsing="false" onAction="#sigIn_click" prefHeight="25.0" prefWidth="80.0" text="Sign In" />
            <Button fx:id="btn_cancel" layoutX="139.0" layoutY="206.0" mnemonicParsing="false" onAction="#cancel_click" prefHeight="25.0" prefWidth="73.0" text="Cancel" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
