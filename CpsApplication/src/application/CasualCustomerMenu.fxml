<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CasualCustomerMenuController">
   <children>
      <Label layoutX="50.0" layoutY="30.0" text="Casual Customer Menu">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <GridPane layoutX="26.0" layoutY="77.0" prefHeight="234.0" prefWidth="307.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="preOrder_btn" mnemonicParsing="false" onAction="#preOrder_click" prefHeight="25.0" prefWidth="133.0" text="Pre Parking Order" />
            <Button fx:id="mailBox_btn" mnemonicParsing="false" onAction="#mailBox_click" prefHeight="25.0" prefWidth="135.0" text="My Mail Box" GridPane.columnIndex="1" />
            <Button fx:id="casualOrder_btn" mnemonicParsing="false" onAction="#casualOrder_click" prefHeight="25.0" prefWidth="133.0" text="Casual Parking Order" GridPane.rowIndex="1" />
            <Button fx:id="sendComplaint_btn" mnemonicParsing="false" onAction="#sendComplaint_click" prefHeight="25.0" prefWidth="137.0" text="Send complaint" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="usePreOrder_btn" mnemonicParsing="false" onAction="#usePreOrder_click" prefHeight="25.0" prefWidth="134.0" text="Use Pre-Order" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button layoutX="113.0" layoutY="311.0" mnemonicParsing="false" onAction="#cancel_click" prefHeight="25.0" prefWidth="133.0" text="Cancel" />
   </children>
</AnchorPane>
